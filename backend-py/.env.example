# PostgreSQL (existing)
DATABASE_URL=postgresql://biolink:biolink_secret@localhost:5432/biolink
PORT=3001
ENVIRONMENT=development

# Ollama models
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=Qwen2.5:7B-Instruct
OLLAMA_ORCHESTRATOR_MODEL=Qwen2.5:7B
OLLAMA_DATA_MODEL=Qwen2.5:7B-Instruct
OLLAMA_MEDICAL_MODEL=alibayram/medgemma:4b
OLLAMA_CODING_MODEL=Qwen2.5:7B-Instruct
LLM_MAX_RETRIES=2
LLM_RETRY_BACKOFF_S=0.4
LLM_RETRY_JITTER_S=0.2
ORCHESTRATOR_LLM_TIMEOUT_S=8.0
DATA_LLM_TIMEOUT_S=20.0
MEDICAL_LLM_TIMEOUT_S=20.0
SQL_AGENT_DEFAULT_LIMIT=200

# RAG / pgvector
RAG_PG_URL=postgresql://biolink:biolink_secret@localhost:5433/biolink_vector
RAG_EMBEDDING_MODEL=nomic-embed-text
RAG_EMBEDDING_DIM=768
RAG_CHUNK_SIZE=800
RAG_CHUNK_OVERLAP=120
RAG_TOP_K=5

# SQL Server (EHVol/BioLink registry)
SQLSERVER_HOST=localhost
SQLSERVER_PORT=1433
SQLSERVER_DB=EHVol
SQLSERVER_USER=readonly_user
SQLSERVER_PASSWORD=readonly_password
SQLSERVER_DRIVER=ODBC Driver 18 for SQL Server
SQLSERVER_TRUST_CERT=true
SQLSERVER_CONNECT_HOST=sqlserver

# Debezium / Kafka
DEBEZIUM_BOOTSTRAP=localhost:9092
DEBEZIUM_TOPIC=EHVol.dbo.patients
CONNECT_URL=http://localhost:8083
